<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="pane" prefHeight="200.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.InsertCategoryView">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="500.0" prefWidth="700.0">
         <children>
            <HBox alignment="CENTER" prefHeight="160.0" prefWidth="700.0">
               <children>
                  <Pane prefHeight="160.0" prefWidth="700.0">
                     <children>
                        <Rectangle fx:id="color1" arcHeight="5.0" arcWidth="5.0" fill="#9c9c9ce5" height="29.0" layoutX="5.0" layoutY="50.0" stroke="BLACK" strokeType="INSIDE" width="690.0" />
                        <Rectangle fx:id="color2" arcHeight="5.0" arcWidth="5.0" fill="#e4f267" height="29.0" layoutX="5.0" layoutY="80.0" stroke="BLACK" strokeType="INSIDE" width="690.0" />
                        <Label fx:id="nameLabel" alignment="CENTER" layoutX="5.0" layoutY="50.0" prefHeight="62.0" prefWidth="690.0" text="TEAM NAME">
                           <font>
                              <Font name="Bauhaus 93" size="54.0" />
                           </font>
                        </Label>
                        <ImageView fx:id="logo" fitHeight="136.0" fitWidth="145.0" layoutX="20.0" layoutY="12.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../img/default/stemma.jpg" />
                           </image>
                        </ImageView>
                     </children>
                  </Pane>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="48.0" prefWidth="700.0">
               <children>
                  <Label text="INSERISCI CATEGORIA" underline="true">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="80.0" prefWidth="700.0" spacing="30.0">
               <children>
                  <Label fx:id="nomeLab" text="Nome*:">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <TextField fx:id="nome" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="80.0" prefWidth="700.0" spacing="20.0">
               <children>
                  <Label fx:id="fotoSqLab" text="Foto squadra:">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <TextField fx:id="fotoSqPath" />
                  <Button fx:id="browseButton" mnemonicParsing="false" onMouseClicked="#oneFileChooser" text="Browse" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="80.0" prefWidth="700.0" spacing="20.0">
               <children>
                  <Button fx:id="insertButton" mnemonicParsing="false" onMouseClicked="#inserisci" text="Inserisci" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
